		</property>
	</bean>

	<bean id="tokenpay6-vir-versionCheckRule" class="com.pay.gateway.validate.crosspay.rule.VersionCheckRule">
		<property name="negativeOutcomeStep">
			<ref bean="tokenpay6-vir-exceptionAction" />
		</property>
		<property name="positiveOutcomeStep">
			<ref local="tokenpay6-vir-partnerIdCheckRule" />
		</property>
		<property name="messageId" value="0002" />
		<property name="message" value="The version number is not correct:版本号不正确" />
	</bean>
	
	   <bean id="tokenpay6-vir-partnerIdCheckRule"
		class="com.pay.gateway.validate.crosspay.rule.PartnerIdCheckRule">
		<property name="negativeOutcomeStep">
			<ref bean="tokenpay6-vir-exceptionAction" />
		</property>
		<property name="positiveOutcomeStep">
			<ref local="tokenpay6-vir-mccCheckRule" />
		</property>
		<property name="messageId" value="0067" />
		<property name="memberService" ref="acc-memberService" />
		<property name="message" value="Merchant(partner ID) is empty or wrong:会员号为空或不正确" />
	</bean>
	
	<bean id="tokenpay6-vir-mccCheckRule" class="com.pay.gateway.validate.crosspay.rule.MccCheckRule">
